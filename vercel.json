{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@vercel/node",
            "config": {
                "nodeVersion": "16.x"
            }
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/server.js"
        }
    ]
}